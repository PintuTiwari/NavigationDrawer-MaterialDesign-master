                  </LinearLayout>-->
                <LinearLayout
                    android:id="@+id/LinearLayout02"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:gravity="right">
                    <!--  <android.support.design.widget.FloatingActionButton
                          android:id="@+id/edit"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          app:fab_size="mini"
                          android:layout_weight="0.9"
                          android:gravity="right"
                          android:layout_gravity="right"
                          app:theme="@style/MyFloatingButton"
                          android:src="@mipmap/ic_edit"
                          app:rippleColor="@color/nliveo_blue_alpha_colorPrimaryDark"
                          app:fab_label="fbgfvbgf" />
  -->
                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/edit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:fab_size="mini"
                        android:src="@mipmap/ic_edit"
                        app:fab_colorNormal="@color/white"
                        app:fab_colorPressed="@color/nliveo_blue_colorAccent"
                        app:fab_colorRipple="@color/nliveo_blue_alpha_colorPrimaryDark"

                        app:fab_label="Menu item 1" />

                    <!--   <android.support.design.widget.FloatingActionButton
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:id="@+id/delete"
                           android:layout_marginLeft="10dp"
                           app:layout_anchor="@+id/appbar"
                           android:layout_weight="0.1"
                           app:theme="@style/MyFloatingButton1"
                           app:fab_size="mini"
                           app:fab_colorPressed="@color/primary_pressed"
                           app:fab_colorRipple="@color/ripple"

                           app:rippleColor="@color/nliveo_blue_alpha_colorPrimaryDark"
                           android:src="@mipmap/cancel">
                   </android.support.design.widget.FloatingActionButton>-->
                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/delete"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:fab_size="mini"
                        android:src="@mipmap/ic_close"
                        app:fab_label="Menu item 1"
                        app:fab_colorNormal="@color/primary"
                        app:fab_colorPressed="@color/primary_dark"
                        app:fab_colorRipple="@color/primary_darker" />

                </LinearLayout>

                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/RelativeLayout01"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/cardtitel1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Name: "
                        android:maxLines="12"
                        android:minLines="1"
                        android:textAppearance="@style/MyTheme.ActionBar.TitleTextStyle"
                        />

                    <TextView
                        android:id="@+id/name_nominee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="Address"
                        android:maxLines="12"
                        android:minLines="1"
                        android:textAppearance="@style/MyTheme.ActionBar.TextStyle"
                        />

                </RelativeLayout>

                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/RelativeLayout02"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Address: "
                        android:textAppearance="@style/MyTheme.ActionBar.TitleTextStyle"
                        />

                    <TextView
                        android:id="@+id/address_nominee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:maxLines="12"
                        android:minLines="1"
                        android:text="Address"
                        android:textAppearance="@style/MyTheme.ActionBar.TextStyle"

                        />

                </RelativeLayout>

                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/RelativeLayout03"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Nominee Relation\nWith Member:"
                        android:textAppearance="@style/MyTheme.ActionBar.TitleTextStyle"

                        />

                    <TextView
                        android:id="@+id/relation_nominee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="Institution Name"
                        android:maxLines="12"
                        android:minLines="1"
                        android:textAppearance="@style/MyTheme.ActionBar.TextStyle"

                        />

                </RelativeLayout>
                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Amount Paid to\nNominee(%):"
                        android:textAppearance="@style/MyTheme.ActionBar.TitleTextStyle"

                        />

                    <TextView
                        android:id="@+id/amountpaid_nominee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="Grade/Class"
                        android:textAppearance="@style/MyTheme.ActionBar.TextStyle" />

                </RelativeLayout>


                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/RelativeLayout04"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/cardtitel4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Date of Birth: "
                        android:textAppearance="@style/MyTheme.ActionBar.TitleTextStyle"

                        />

                    <TextView
                        android:id="@+id/dob"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="Grade/Class"
                        android:textAppearance="@style/MyTheme.ActionBar.TextStyle" />

                </RelativeLayout>

                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/RelativeLayout05"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="5dp">

                        <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:maxLines="12"
                        android:minLines="1"
                        android:text="Name and Address\n of Guardian: "
                        android:textAppearance="@style/MyTheme.ActionBar.TitleTextStyle" />

                    <TextView
                        android:id="@+id/address_guardin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:maxLines="12"
                        android:minLines="1"
                        android:text="Grade/Class"
                        android:textAppearance="@style/MyTheme.ActionBar.TextStyle" />

                </RelativeLayout>

                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/RelativeLayout06"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="5dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Age: "
                        android:textAppearance="@style/MyTheme.ActionBar.TitleTextStyle" />

                    <TextView
                        android:id="@+id/age_nominee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:text="Grade/Class	: "
                        android:textAppearance="@style/MyTheme.ActionBar.TextStyle" />

                </RelativeLayout>


            </LinearLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>

</LinearLayout>

