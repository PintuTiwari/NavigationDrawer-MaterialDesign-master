    android:layout_width="match_parent"
    android:id="@+id/frame_view_viewacademic"
    android:layout_height="match_parent">
    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyle_viewacademic"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.github.clans.fab.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/menu1"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="10dp"
        app:layout_anchor="@+id/appbar"
        android:src="@mipmap/ic_add_white_24dp"
        app:fab_colorNormal="@color/nliveo_blue_colorPrimary"
        app:fab_colorPressed="@color/nliveo_blue_colorAccent"
        app:fab_colorRipple="@color/nliveo_blue_colorPrimaryDark"
        android:layout_gravity="bottom|right"/>


